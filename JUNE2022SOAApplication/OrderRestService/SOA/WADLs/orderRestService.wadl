<?xml version = '1.0' encoding = 'UTF-8'?>
<application xmlns:soa="http://www.oracle.com/soa/rest" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:ns0="http://pratice.com/order/create/request" xmlns="http://wadl.dev.java.net/2009/02">
   <doc title="orderRestService">RestService</doc>
   <grammars>
      <xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
            <xsd:import namespace="http://pratice.com/order/create/request" schemaLocation="../Schemas/createOrderRequest.xsd"/>
        </xsd:schema>
      <xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
            <xsd:import namespace="http://practice.com/order/create/response" schemaLocation="../Schemas/createOrderResponse.xsd"/>
        </xsd:schema>
   </grammars>
   <resources>
      <resource path="/order/create">
         <method name="POST" soa:wsdlOperation="createOperation">
            <request>
               <representation mediaType="application/json" element="cns:orderCreateRequest" xmlns:cns="http://pratice.com/order/create/request"/>
            </request>
            <response status="200">
               <representation mediaType="application/json" element="cns:createOrderResponse" xmlns:cns="http://practice.com/order/create/response"/>
            </response>
         </method>
      </resource>
   </resources>
</application>
